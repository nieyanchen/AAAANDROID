        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".RateCalcActivity" android:theme="@style/Theme.AppCompat.Light.Dialog"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".SecondActivity" />
        <activity android:name=".RateActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ConfgActivity" />
        <activity android:name=".RateListActivity" />
        <activity android:name=".MyListActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyList2Activity" />
    </application>

</manifest>